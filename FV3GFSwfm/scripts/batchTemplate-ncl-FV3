# UDUNITS=`which udunits`
# export UDUNITS2_XML_PATH=${NCARG_ROOT}/lib/ncarg/udunits/udunits2.xml

echo "NCARG_ROOT: $NCARG_ROOT"

# Set up paths to shell commands
LS=/bin/ls
LN=/bin/ln
RM=/bin/rm
MKDIR="/bin/mkdir -m 775"
CP=/bin/cp
MV=/bin/mv
ECHO=/bin/echo
