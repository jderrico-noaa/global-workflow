yyjjjhhmm_BACK12=$( date +"%y%j%H%M" -d "${yyyymmddhh_BACK12}" )
yyjjjhhmm_BACK24=$( date +"%y%j%H%M" -d "${yyyymmddhh_BACK24}" )
yyyymmddhh_BACK12=$( date +"%Y%m%d%H" -d "${yyyymmddhh_BACK12}" )
yyyymmddhh_BACK24=$( date +"%Y%m%d%H" -d "${yyyymmddhh_BACK24}" )
