FCST_TIME_BACK6=-999
FCST_TIME_BACK12=-999
FCST_TIME_BACK24=-999
if (( ${FCST_TIME} >= 6 )); then
  FCST_TIME_BACK6=$(($FCST_TIME - 6))
fi
if (( ${FCST_TIME} >= 12 )); then
  FCST_TIME_BACK12=$(($FCST_TIME - 12))
fi
if (( ${FCST_TIME} >= 24 )); then
  FCST_TIME_BACK24=$(($FCST_TIME - 24))
