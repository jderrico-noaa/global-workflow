back12hr=`echo $yyyymmddhh_BACK12 | cut -c9-10`
back24ymd=`echo $yyyymmddhh_BACK24 | cut -c1-8`
back24hr=`echo $yyyymmddhh_BACK24 | cut -c9-10`
