

rocotorun -w p8-chem.xml -d p8-chem.db
rocotostat -w p8-chem.xml -d p8-chem.db

## create initial conditions
rocotorun -w chemics.xml -d chemics.db
rocotostat -w chemics.xml -d chemics.db

## coupled_ic task -- copy ICS from ICSDIR to ROTDIR
rocotorun -w cplic.xml -d cplic.db
rocotostat -w cplic.xml -d cplic.db

## PyGraf images
rocotorun -w pygraf_p8-chem.xml -d pygraf_p8-chem.db
rocotostat -w pygraf_p8-chem.xml -d pygraf_p8-chem.db

rocotorun -w exp-pygraf.xml -d exp-pygraf.db
rocotostat -w exp-pygraf.xml -d exp-pygraf.db
